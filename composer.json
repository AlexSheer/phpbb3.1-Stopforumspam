{
    "name": "sheer/stopforumspam",
    "type": "phpbb-extension",
    "description": "Allow find spamers in you board using API stopforumspam.com",
    "homepage": "https://phpbbguru.net",
    "version": "1.0.0",
    "time": "2018-11-11",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Sheer",
            "homepage": "https://phpbbguru.net",
            "role": "Support team"
        }
    ],
    "require": {
        "php": ">=5.3.3"
    },
    "extra": {
        "display-name": "Spamer Finder",
        "soft-require": {
            "phpbb/phpbb": "3.2.*@dev"
        },
        "version-check": {
            "host": "sheer.phpbbguru.net",
            "ssl": true,
            "directory": "/versions/stopforumspam",
            "filename": "version.json"
        }
    }
}
